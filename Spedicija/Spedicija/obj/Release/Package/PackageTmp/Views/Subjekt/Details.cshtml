@model Spedicija.Subjekt

@{
    ViewBag.Title = "Details";
}



<section>       
<div class="container"> 
    <div class="section-body ">
          
        <div class="card card-bordered style-primary">
            <div class="card-head">
                			    <div class="tools">
								<div class="btn-group">
	                            <a class="btn btn-icon-toggle btn-refresh"><i class="md md-refresh"></i></a>
	                            <a class="btn btn-icon-toggle btn-collapse"><i class="fa fa-angle-down"></i></a>
	                            <a class="btn btn-icon-toggle btn-close"><i class="md md-close"></i></a>
                                </div>
							    </div>
							    <header><i class="fa fa-fw fa-list"></i>&nbsp Detalji</header>
			</div>
            <div class="card-body style-default-bright">

			<ul class="list divider-full-bleed">
	


	<li class="tile">
		<div class="ink-reaction">
			<div class="row">
			<div class="col-md-3"><div class="tile-icon">@Html.DisplayNameFor(model => model.Naziv)</div><!-- <i class="fa fa-inbox"></i> --> </div>
						<div class="col-md-9"><div class="tile-text text-bold">@Html.DisplayFor(model => model.Naziv)</div></div>
						</div>
		</div>
	</li>

	

	<li class="tile">
		<div class="ink-reaction">
			<div class="row">
			<div class="col-md-3"><div class="tile-icon">@Html.DisplayNameFor(model => model.Adresa)</div><!-- <i class="fa fa-inbox"></i> --> </div>
						<div class="col-md-9"><div class="tile-text text-bold">@Html.DisplayFor(model => model.Adresa) @Html.DisplayFor(model => model.PTT) @Html.DisplayFor(model => model.Grad) </div></div>
						</div>
		</div>
	</li>
                <li class="tile">
                    <div class="ink-reaction">
                        <div class="row">
                            <div class="col-md-3"><div class="tile-icon">@Html.DisplayNameFor(model => model.Drzava)</div><!-- <i class="fa fa-inbox"></i> --> </div>
                            <div class="col-md-9"><div class="tile-text text-bold">@Html.DisplayFor(model => model.Drzava)  </div></div>
                        </div>
                    </div>
                </li>

	

	<li class="tile">
		<div class="ink-reaction">
			<div class="row">
			<div class="col-md-3"><div class="tile-icon">@Html.DisplayNameFor(model => model.Telefon)</div><!-- <i class="fa fa-inbox"></i> --> </div>
						<div class="col-md-9"><div class="tile-text text-bold">@Html.DisplayFor(model => model.Telefon)</div></div>
						</div>
		</div>
	</li>

	

	<li class="tile">
		<div class="ink-reaction">
			<div class="row">
			<div class="col-md-3"><div class="tile-icon">@Html.DisplayNameFor(model => model.Email)</div><!-- <i class="fa fa-inbox"></i> --> </div>
						<div class="col-md-9"><div class="tile-text text-bold">@Html.DisplayFor(model => model.Email)</div></div>
						</div>
		</div>
	</li>

	

	<li class="tile">
		<div class="ink-reaction">
			<div class="row">
			<div class="col-md-3"><div class="tile-icon">@Html.DisplayNameFor(model => model.KontaktOsoba)</div><!-- <i class="fa fa-inbox"></i> --> </div>
						<div class="col-md-9"><div class="tile-text text-bold">@Html.DisplayFor(model => model.KontaktOsoba)</div></div>
						</div>
		</div>
	</li>

	

	<li class="tile">
		<div class="ink-reaction">
			<div class="row">
			<div class="col-md-3"><div class="tile-icon">@Html.DisplayNameFor(model => model.Timocom)</div><!-- <i class="fa fa-inbox"></i> --> </div>
						<div class="col-md-9"><div class="tile-text text-bold">@Html.DisplayFor(model => model.Timocom)</div></div>
						</div>
		</div>
	</li>

	

	<li class="tile">
		<div class="ink-reaction">
			<div class="row">
			<div class="col-md-3"><div class="tile-icon">@Html.DisplayNameFor(model => model.DatumKreiranja)</div><!-- <i class="fa fa-inbox"></i> --> </div>
						<div class="col-md-9"><div class="tile-text text-bold">@Html.DisplayFor(model => model.DatumKreiranja , "ShortDateTime" )</div></div>
						</div>
		</div>
	</li>

	

	<li class="tile">
		<div class="ink-reaction">
			<div class="row">
			<div class="col-md-3"><div class="tile-icon">@Html.DisplayNameFor(model => model.DatumZapisa)</div><!-- <i class="fa fa-inbox"></i> --> </div>
						<div class="col-md-9"><div class="tile-text text-bold">@Html.DisplayFor(model => model.DatumZapisa  )</div></div>
						</div>
		</div>
	</li>

	

	<li class="tile">
		<div class="ink-reaction">
			<div class="row">
			<div class="col-md-3"><div class="tile-icon">@Html.DisplayNameFor(model => model.ZapisAktivan)</div><!-- <i class="fa fa-inbox"></i> --> </div>
						<div class="col-md-9"><div class="tile-text text-bold"@if (Model.ZapisAktivan ) { <span>DA</span> } else {  <span>NE</span> } 
                             
                            </div></div>
						</div>
		</div>
	</li>

	

	<li class="tile">
		<div class="ink-reaction">
			<div class="row">
			<div class="col-md-3"><div class="tile-icon">@Html.DisplayNameFor(model => model.Opis)</div><!-- <i class="fa fa-inbox"></i> --> </div>
						<div class="col-md-9"><div class="tile-text text-bold">@Html.DisplayFor(model => model.Opis)</div></div>
						</div>
		</div>
	</li>

                <li class="tile">
                    <div class="ink-reaction">
                        <div class="row">
                            <div class="col-md-3"><div class="tile-icon">@Html.DisplayNameFor(model => model.JIB)</div><!-- <i class="fa fa-inbox"></i> --> </div>
                            <div class="col-md-9"><div class="tile-text text-bold">@Html.DisplayFor(model => model.JIB)</div></div>
                        </div>
                    </div>
                </li>
                
	<li class="tile">
		<div class="ink-reaction">
			<div class="row">
			<div class="col-md-3"><div class="tile-icon">@Html.DisplayNameFor(model => model.PIB)</div><!-- <i class="fa fa-inbox"></i> --> </div>
						<div class="col-md-9"><div class="tile-text text-bold">@Html.DisplayFor(model => model.PIB)</div></div>
						</div>
		</div>
	</li>

</ul>
</div>
          <div class="card-actionbar style-default-bright">
				<div class="card-actionbar-row">	
										    @Html.ActionLink("IZMJENI", "Edit", new {  id=Model.IdSubjekt  } , new { @class = "btn btn-flat btn-primary ink-reaction"  }) 
					    @Html.ActionLink("NAZAD NA LISTU", "Index", null, new { @class = "btn btn-flat btn-primary ink-reaction"  })
									</div>
			</div>
</div>
</div>
        </div>
    </section>


