		 Izvoznik : {
				"required" : true

					}
 
				,
		 IzvoznikKontaktOsoba : {
				"required" : true

					}
 
				,
		 MjestoUtovara : {
				"required" : true

					}
 
				,
		 DatumUtovara : {
				"required" : true

					}
 
				,
		 ReferentniBrojUtovara : {
				"required" : true

					}
 
				,
		 VrstaRobe : {
