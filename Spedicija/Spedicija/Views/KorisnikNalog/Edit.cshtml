					}
 
				,
		 BrutoTezina : {
				"required" : true

					}
 
				,
		 IzvoznaCarina : {
				"required" : true

					}
 
				,
		 Uvoznik : {
				"required" : true

					}
 
				,
		 UvoznikKontaktOsoba : {
				"required" : true

					}
 
				,
		 MjestoIstovara : {
				"required" : true

					}
 
				,
		 DatumIstovara : {
				"required" : true

					}
 
				,
		 UvoznaCarina : {
				"required" : true

					}
 
				,
		 DatumZahtjeva : {
				"required" : true

					}
						}
						 });

        $('submit').on('click', function (e) {
            e.preventDefault();

            if (!($(".form").valid())) {
                validator.focusInvalid();
            }
			else $(".form").submit();


        });
  });
</script> 

