        $("#UtovarFirma").focusout(function () {
            if ($("#UtovarAdresa").val() == '' || $("#UtovarGrad").val() == '') {
                $.getJSON("/DnevnikPrevoza/AjaxVratiAdresuFirme", { Firma: $("#UtovarFirma").val() }, function (data) {
                    if (data.Adresa != "") {
                        $("#UtovarAdresa").val(data.Adresa);
