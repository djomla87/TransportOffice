                      $.getJSON('/DnevnikPrevoza/ObavezeDanas', function (data) {
                          console.log("ObavezeDanas");
                          $.each(data, function (i, obaveze) {
                              count++;

                              items += "<li><a class='alert alert-callout alert-warning' href='/DnevnikPrevoza/Details/" + obaveze.IdDnevnik + "'>";
