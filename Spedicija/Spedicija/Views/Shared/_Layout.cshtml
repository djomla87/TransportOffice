                    $("#badge-valute").removeClass("style-success");
                    $("#badge-valute").addClass("style-danger");
                    $("#badge-valute").html(data);
                          }
                          else {
                    $("#badge-valute").removeClass("style-danger");
                    $("#badge-valute").addClass("style-success");
                    $("#badge-valute").html(data);
                          }


                      });
                  
            }


                  var items = "<li class='dropdown-header'>Današnje Obaveze</li>";
